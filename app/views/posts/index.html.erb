<!-- The Timeline Section -->
<div class="row"> <!--row containing all timeline content-->
  <section class="col-md-5"> <!--left column-->
      <%= render partial: "about" %>

      <%= render partial: "photos" %>

      <%= render partial: "friends" %>

  </section> <!--end left column-->

  <section class="col-md-7"> <!--right column-->

    <%= render partial: "form" %>

    <!--List of Posts-->

    <% @posts.each do |post|

      <%= render partial: "post", locals: { post: post } %>

    <% end %>

  </section> <!--end right column-->
</div> <!--end row containing all timeline content-->
